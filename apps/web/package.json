{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "test:refactorisation": "jest --testPathPattern='refactorisation|components|utils|constants'",
    "test:performance": "playwright test --grep='Performance'",
    "test:regression": "jest --testNamePattern='regression'",
    "test:api": "jest --testPathPattern='api'",
    "test:components": "jest --testPathPattern='components'",
    "test:utils": "jest --testPathPattern='utils'",
    "playwright:install": "playwright install"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.98",
    "@anthropic-ai/sdk": "^0.60.0",
    "@mdxeditor/editor": "^3.42.0",
    "@monaco-editor/react": "^4.6.0",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.38.5",
    "@tiptap/extension-blockquote": "^3.3.0",
    "@tiptap/extension-bold": "^3.3.0",
    "@tiptap/extension-code-block": "^3.3.0",
    "@tiptap/extension-heading": "^3.3.0",
    "@tiptap/extension-image": "^3.3.0",
    "@tiptap/extension-italic": "^3.3.0",
    "@tiptap/extension-link": "^3.3.0",
    "@tiptap/extension-list-item": "^3.3.0",
    "@tiptap/pm": "^3.3.0",
    "@tiptap/react": "^3.3.0",
    "@tiptap/starter-kit": "^3.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vercel/analytics": "^1.5.0",
    "next": "15.4.7",
    "react": "19.0.0",
    "react-arborist": "^3.4.3",
    "react-dom": "19.0.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.4",
    "react-syntax-highlighter": "^15.6.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "15.4.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
